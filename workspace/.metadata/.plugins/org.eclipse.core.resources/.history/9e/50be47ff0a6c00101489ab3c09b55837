package com.app.ex;

import java.io.IOException;

import jakarta.servlet.ServletException;
import jakarta.servlet.http.HttpServlet;
import jakarta.servlet.http.HttpServletRequest;
import jakarta.servlet.http.HttpServletResponse;

public class Ex04Result extends HttpServlet {
	@Override
	protected void doGet(HttpServletRequest req, HttpServletResponse resp) throws ServletException, IOException {
			String firstLevelDivision = req.getParameter("firstLevelDivision");
			String printAddress = "";
			req.setAttribute("firstLevelDivision", firstLevelDivision);
			req.setAttribute("fruitPrice", fruitPrice);
			req.setAttribute("fruitInfo", fruitInfo);
			req.getRequestDispatcher("/ex03-result.jsp").forward(req, resp);
	}

}
